	<VARIABLES>SABC</VARIABLES>

	<STARTVAR>S</STARTVAR>

	<PRODUCTIONS>

		<HEAD var="S">
			<BODY>AB</BODY>
		</HEAD>

		<HEAD var="S">
			<BODY>BC</BODY>
		</HEAD>

		<HEAD var="A">
			<BODY>BA</BODY>
		</HEAD>

		<HEAD var="A">
			<BODY>a</BODY>
		</HEAD>

		<HEAD var="B">
			<BODY>b</BODY>
		</HEAD>

		<HEAD var="B">
            <BODY>CC</BODY>
    </HEAD>

		<HEAD var="C">
			<BODY>AB</BODY>
		</HEAD>

		<HEAD var="C">
			<BODY>a</BODY>
		</HEAD>


	</PRODUCTIONS>

</CFG>
